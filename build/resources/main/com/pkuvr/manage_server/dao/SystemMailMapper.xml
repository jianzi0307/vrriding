        where Available=true and Global=true and
        Game_ID=#{gameId,jdbcType=INTEGER} and (BeginDate is null or BeginDate&lt;=CURRENT_TIMESTAMP)
        AND (End_Time IS NULL OR End_Time >= CURRENT_TIMESTAMP)
        UNION all
        select mail.* from System_Mail mail,System_Mail_To_Server toServer
        where mail.Mail_ID=toServer.Mail_ID and Available=true and Global=false and
        Game_ID=#{gameId,jdbcType=INTEGER} and
        toServer.Server_ID=#{serverId,jdbcType=VARCHAR} and (BeginDate is null or BeginDate&lt;=CURRENT_TIMESTAMP)
        AND (End_Time IS NULL OR End_Time >= CURRENT_TIMESTAMP)) mailAll
        order by mailAll.Add_Time desc
